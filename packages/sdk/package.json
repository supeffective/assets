{
  "name": "@supereffectivegg/assets-sdk",
  "version": "0.1.0",
  "description": "SuperEffective.gg PokÃ©mon Assets SDK",
  "keywords": [
    "modular",
    "typescript",
    "pokemon",
    "pokeapi",
    "type-safe"
  ],
  "homepage": "https://supereffective.gg",
  "repository": {
    "type": "git",
    "url": "https://github.com/itsjavi/supereffective-assets"
  },
  "license": "MIT",
  "author": "Javier Aguilar",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --noEmit && tsup",
    "postinstall": "pnpm run build",
    "prepublish": "pnpm run build",
    "version:fix": "npm version patch",
    "version:major": "npm version major",
    "version:minor": "npm version minor"
  },
  "dependencies": {
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
