{
  "name": "@supereffectivegg/assets-sdk",
  "version": "0.5.0",
  "description": "SuperEffective.gg PokÃ©mon Assets SDK",
  "keywords": [
    "modular",
    "typescript",
    "pokemon",
    "pokeapi",
    "type-safe"
  ],
  "homepage": "https://supereffective.gg",
  "repository": {
    "type": "git",
    "url": "https://github.com/itsjavi/supereffective-assets"
  },
  "license": "MIT",
  "author": "Javier Aguilar",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js",
      "default": "./dist/node.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --dts --clean",
    "postinstall": "pnpm run build",
    "prepublish": "pnpm run build"
  },
  "dependencies": {
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^20.4.8",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
