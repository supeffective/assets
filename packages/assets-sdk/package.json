{
  "name": "@supeffective/assets-sdk",
  "version": "4.1.3",
  "description": "SuperEffective.gg Pok√©mon Assets SDK",
  "keywords": [
    "modular",
    "typescript",
    "pokemon",
    "pokeapi",
    "pokedex",
    "pokedex-tracker",
    "type-safe"
  ],
  "homepage": "https://github.com/itsjavi/supereffective-assets#readme",
  "bugs": {
    "url": "https://github.com/itsjavi/supereffective-assets/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/itsjavi/supereffective-assets.git"
  },
  "license": "MIT",
  "author": "Javier Aguilar",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --clean=false",
    "lint": "eslint --max-warnings=0 .",
    "lint-fix": "eslint --fix .",
    "publint-check": "pnpm publint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "eslint": "8.50.0",
    "publint": "^0.2.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
